    "csrf": "^3.1.0",
    "debug": "^4.3.1",
    "deprecate": "^1.0.0",
    "dotenv": "^4.0.0",
    "duplexify": "^3.5.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-partial-templates": "^0.2.0",
    "express-session": "^1.13.0",
    "findup": "^0.1.5",
    "glob": "^7.2.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "3.14",
    "govuk_template_mustache": "^0.26.0",
    "helmet": "^3.22.0",
    "hof": "file:.yalc/hof",
